/efs			ext4		/dev/block/mmcblk0p15                          
/boot			emmc		/dev/block/mmcblk0p5
/recovery		emmc		/dev/block/mmcblk0p6
/cache			ext4		/dev/block/mmcblk0p16
/system			ext4		/dev/block/mmcblk0p17
/data			ext4		/dev/block/mmcblk0p19	
/preload		ext4		/dev/block/mmcblk0p18                     
/sdcard			vfat		/dev/block/mmcblk1p1	 
/modem			emmc		/dev/block/mmcblk0p7
